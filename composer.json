{
    "name": "emanueleminotto/jshrink-service-provider",
    "description": "Jshrink service provider for Silex 1 based on nibsirahsieu/SalvaJshrinkBundle.",
    "keywords": [
        "Assetic",
        "javascript",
        "jshrink",
        "minify",
        "silex",
        "twig"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Emanuele Minotto",
            "email": "minottoemanuele@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "salva/jshrink-bundle": "^1.1.1",
        "silex/silex": "~1"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4.0",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "EmanueleMinotto\\JshrinkServiceProvider\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmanueleMinotto\\JshrinkServiceProvider\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
